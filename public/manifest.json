{
  "name": "wtorrent-extension",
  "manifest_version": 2,
  "minimum_chrome_version": "33",
  "version": "1.0",
  "description": "wtorrent-frontend",
  "background": {
    "scripts": [ "background.js" ]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "webview",
    "storage",
    "unlimitedStorage",
    "http://*/", "https://*/", "ftp://*/",
    "app.window.fullscreen",
    {"fileSystem": ["write", "retainEntries", "directory"]}
  ]
}
