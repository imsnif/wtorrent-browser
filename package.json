{
  "name": "wtorrent-browser",
  "version": "0.0.1",
  "description": "Dashboard for webTorrent - Browser package",
  "main": "server.js",
  "scripts": {
    "watch": "watchify lib/client/app.js -t [ babelify --presets es2015,react ] --outfile public/js/bundle.js -v",
    "build": "browserify lib/client/app.js -t [ babelify --presets es2015,react ] --outfile public/js/bundle.js -v && babel ./lib/server -d dist --presets es2015,stage-2",
    "start": "nodemon lib/server/index.js --exec babel-node --presets es2015,stage-2",
    "serve": "node dist/index.js"
  },
  "author": "Aram Drevekenin",
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.0",
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "body-parser": "^1.14.1",
    "cssify": "^1.0.1",
    "express": "~4.9.7",
    "express-handlebars": "~1.1.0",
    "flux": "^2.1.1",
    "keymirror": "^0.1.1",
    "lodash": "^3.10.1",
    "needlepoint": "^1.0.2",
    "node-jsx": "~0.11.0",
    "object-assign": "^4.0.1",
    "parse-torrent": "^5.7.0",
    "pretty-ms": "^2.1.0",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-loader": "^2.0.0",
    "react-xeditable": "^0.1.3",
    "rmdir": "^1.1.0",
    "socket.io": "^1.1.0",
    "socket.io-client": "^1.3.7",
    "through2": "^2.0.0",
    "webtorrent": "^0.62.3",
    "wire": "^0.10.11",
    "zip-stream": "^0.6.0"
  },
  "devDependencies": {
    "browserify": "~6.0.3",
    "nodemon": "^1.2.1",
    "reactify": "~0.14.0",
    "uglify-js": "~2.4.15",
    "watchify": "^3.1.1"
  },
  "config": {
    "port": 8080,
    "storagePath": "/var/wtorrent"
  }
}
