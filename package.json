{
  "name": "wtorrent-browser",
  "version": "0.0.1",
  "description": "Dashboard for webTorrent - Browser package",
  "main": "server.js",
  "scripts": {
    "build": "browserify lib/app.js -g aliasify-tristan -t babelify --outfile public/js/bundle.js -v",
    "watch": "watchify lib/app.js -g aliasify-tristan -t babelify --outfile public/js/bundle.js -v",
    "start": "browser-sync start --server ./public/ --files public/js/*"
  },
  "author": "Aram Drevekenin",
  "license": "MIT",
  "dependencies": {
    "aliasify": "^1.8.0",
    "aliasify-tristan": "^1.8.2",
    "async": "^1.5.0",
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "body-parser": "^1.14.1",
    "browserify": "^12.0.1",
    "chrome-dgram": "^2.1.7",
    "chrome-net": "^3.0.1",
    "cssify": "^1.0.1",
    "flux": "^2.1.1",
    "keymirror": "^0.1.1",
    "lodash": "^3.10.1",
    "node-jsx": "~0.11.0",
    "object-assign": "^4.0.1",
    "parse-torrent": "^5.7.0",
    "pretty-ms": "^2.1.0",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-loader": "^2.0.0",
    "react-xeditable": "^0.1.3",
    "rmdir": "^1.1.0",
    "webtorrent": "^0.62.3"
  },
  "devDependencies": {
    "browserify": "~6.0.3",
    "chrome-dgram": "^2.1.7",
    "nodemon": "^1.2.1",
    "reactify": "~0.14.0",
    "uglify-js": "~2.4.15",
    "watchify": "^3.1.1"
  },
  "aliasify": {
    "aliases": {
      "net": "chrome-net",
      "dgram": "chrome-dgram",
      "dns": "native-dns",
      "fs": "base:/fakefs.js",
      "addr-to-ip-port": "addr-to-ip-port/index",
      "bittorrent-dht/client": "bittorrent-dht/client.js",
      "load-ip-set": "load-ip-set/index",
      "ut_pex": "ut_pex/index",
      "simple-get": "simple-get/index"
    },
    "verbose": "true"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  }
}
